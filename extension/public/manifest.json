{
  "manifest_version": 3,
  "name": "ai extension",
  "description": "ai extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content.js"]
  }],
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "host_permissions": [
    "https://swapi.dev/*"
  ]
}